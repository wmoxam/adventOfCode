inputs = [] of String
outputs = [] of String

n = 0

ARGF.gets_to_end.each_line do |input|
  input, output = input.split(/\|/)
  inputs = input.split(/\s+/)
  outputs = output.split(/\s+/)
  n += outputs.select { |s| [2, 3, 4, 7].includes?(s.size) }.size
end

puts n
